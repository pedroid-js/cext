{
  "name": "customScriptsExt",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Custom scripts ext",
  "icons": {
      "512": "logo512.png"
  },
  "browser_action": {
      "default_icon": "logo512.png",
      "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' *://*; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "storage"
  ]
}